

  <div class="row">
  <h4>New User Registration</h4>
    <form class="col s12" method="post" action="/users">
      <div class="row">
        <div class="input-field col s6">
          <i class="material-icons prefix">account_circle</i>
          <input id="icon_prefix" type="text" name="user[email]" class="validate">
          <label for="icon_prefix">First Name</label>
        </div>
        <div class="input-field col s6">
          <i class="material-icons prefix">lock_outline</i>
          <input id="icon_password" type="tel" name="user[password]" class="validate">
          <label for="icon_password">Password</label>
        </div>
      </div>
      <input class="waves-effect waves-light btn" type="submit" value="Register">
    </form>
  </div>
